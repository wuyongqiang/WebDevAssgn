<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
     namespace="PersistData" assembly="PersistData">

  <class
      name="TOrderType"
      table="OrderType"
      lazy="true">

    <id
        name="Id"
        column="ORDERTYPE_ID">

      <generator class="native"/>

    </id>

    <property
        name="Text"
        column="ORDERTYPE_TEXT"
        not-null="true"
        unique="false"/>

   

  </class>

</hibernate-mapping>
